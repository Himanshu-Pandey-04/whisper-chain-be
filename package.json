    "commander": "7.1.0",
    "continuation-local-storage": "3.2.1",
    "cookie-parser": "1.4.5",
    "express": "4.17.1",
    "google-libphonenumber": "3.2.22",
    "helmet": "4.4.1",
    "html-entities": "2.1.0",
    "js-base64": "3.7.2",
    "moment-timezone": "0.5.33",
    "morgan": "1.10.0",
    "mysql": "2.18.1",
    "sanitize-html": "2.3.2",
    "socket.io": "3.1.1",
    "uuid": "8.3.2",
    "swagger-jsdoc": "6.2.1",
    "swagger-ui-express": "4.3.0"
  },
  "devDependencies": {
    "eslint": "7.20.0",
    "lint-staged": "10.5.4",
